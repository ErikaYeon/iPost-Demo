{
  "expo": {
    "name": "iPost",
    "slug": "iPost",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icons/LogoiPost.png",
    "scheme": "ipost",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/icons/ic_launcher_foreground.png",
      "resizeMode": "resizeMode",
      "backgroundColor": "#201E43"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icons/ic_launcher_foreground.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ipost.iPost"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icons/LogoiPost.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "eaf79ae7-5e48-4743-9472-c25fb37df7d0"
      }
    }
  }
}
